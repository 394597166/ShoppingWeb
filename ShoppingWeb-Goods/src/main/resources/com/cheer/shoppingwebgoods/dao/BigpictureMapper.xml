<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
        <!--namespace对应的dao接口-->
<mapper namespace="com.cheer.shoppingwebgoods.dao.BigpictureMapper">
    <!-- id是方法名 #{id}是方法参数 -->
    <select id="selectBigPictureAndPictureId" resultType="com.cheer.shoppingwebgoods.model.Bigpicture">
        select * from bigpicture where pictureId = #{pictureId}
    </select>
</mapper>
